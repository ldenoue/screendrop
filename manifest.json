{
  "name": "ScreenDrop",
  "author": "Laurent Denoue",
  "manifest_version": 2,
  "version": "1.0.4",
  "description": "Quickly record a video of your screen or window with your voice, drag & drop anywhere",
  "background": {"scripts": ["sd-background.js"] },
  "permissions": ["downloads", "downloads.open", "storage"],
  "browser_action": {
		"default_title": "ScreenDrop",
  	"default_icon": "icons/off.png"
	},
  "options_page": "welcome.html",
  "icons": {
    "128": "icons/off.png"
  }
}
